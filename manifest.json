{
  "manifest_version": 2,
  "name": "RhinoFit ComPort Communicator",
  "description": "Send open command to turnstile or door",
  "short_name": "ComPort Kiosk",
  "version": "1.2",
  "icons": {
    "16": "rhino-16.png",
    "128": "rhino-128.png"
  },
  "app": {
    "background": {
        "scripts":  ["background.js", "jquery-1.11.0.min.js"]
    }
  },
  "permissions": [
    "background",
    "storage",
    "serial"
  ]
}